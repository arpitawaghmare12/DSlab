#include <stdio.h>
#include <string.h>

void main() {
    char a[20][20];
    int i, j, n;
    char temp[20];

    printf("Enter value of n: ");
    scanf("%d", &n);

    // Read n strings
    for (i = 0; i < n; i++) {
        printf("Enter string for a[%d]: ", i);
        scanf("%s", a[i]);
    }

    printf("Before sorting, the strings in the array are:\n");
    // Print strings before sorting
    for (i = 0; i < n; i++) {
        printf("String at a[%d] = %s\n", i, a[i]);
    }

    // Sort the strings using bubble sort
    for (i = 0; i < n - 1; i++) {
        for (j = 0; j < n - 1 - i; j++) {
            if (strcmp(a[j], a[j + 1]) > 0) {
                strcpy(temp, a[j]);
                strcpy(a[j], a[j + 1]);
                strcpy(a[j + 1], temp);
            }
        }
    }

    printf("After sorting, the strings in the array are:\n");
    // Print strings after sorting
    for (i = 0; i < n; i++) {
        printf("String at a[%d] = %s\n", i, a[i]);
    }
}
